Options +FollowSymlinks
RewriteEngine on
RewriteRule ^index.php/([^/]+)/([^/]+) index.php?page=$1&id=$2 [NC]
RewriteRule ^index.php/([^/]+) index.php?page=$1 [NC]

<FilesMatch "\.php$">
    Order Allow,Deny
    Deny from all
</FilesMatch>
<FilesMatch "index[0-9]?\.php$">
    Order Allow,Deny
    Allow from all
</FilesMatch>